/**
 * Implements hook_entity_type_build().
 */
function farm_pcsc_entity_type_build(array &$entity_types) {
  // Remove after https://github.com/farmOS/farmOS/pull/818 is merged.
  /** @var \Drupal\Core\Entity\EntityTypeInterface[] $entity_types */
  if (!empty($entity_types['plan_record'])) {

    // Unset the data table.
    $entity_types['plan_record']->set('data_table', NULL);

    // Set the views data handler class to FarmEntityViewsData.
    $entity_types['plan_record']->setHandlerClass('views_data', FarmEntityViewsData::class);

    $entity_types['plan_record']->setHandlerClass('view_builder',EntityViewBuilder::class);
    $entity_types['plan_record']->setHandlerClass('local_task_provider', [
      'default' => DefaultEntityLocalTaskProvider::class,
    ]);
    $entity_types['plan_record']->setLinkTemplate('canonical', '/plan/record/{plan_record}');
  }
}

/**
 * Define bundle classes for plan records.
 *
 * @return array
 *   Returns an array of bundle classes keyed by bundle ID.
 */
function farm_pcsc_plan_record_bundle_classes() {
  return [
    'pcsc_field' => PcscField::class,
    'pcsc_commodity' => PcscCommodity::class,
    'pcsc_farm_summary' => PcscFarmSummary::class,
    'pcsc_field_summary' => PcscFieldSummary::class,
    'pcsc_field_practice_327' => PcscFieldPractice327::class,
    'pcsc_field_practice_328' => PcscFieldPractice328::class,
    'pcsc_field_practice_329' => PcscFieldPractice329::class,
    'pcsc_field_practice_336' => PcscFieldPractice336::class,
    'pcsc_field_practice_340' => PcscFieldPractice340::class,
    'pcsc_field_practice_345' => PcscFieldPractice345::class,
    'pcsc_field_practice_484' => PcscFieldPractice484::class,
    'pcsc_field_practice_528' => PcscFieldPractice528::class,
    'pcsc_field_practice_590' => PcscFieldPractice590::class,
  ];
}

/**
 * Implements hook_entity_bundle_info_alter().
 */
function farm_pcsc_entity_bundle_info_alter(array &$bundles): void {
  foreach (farm_pcsc_plan_record_bundle_classes() as $bundle => $class) {
    if (isset($bundles['plan_record'][$bundle])) {
      $bundles['plan_record'][$bundle]['class'] = $class;
    }
  }
}

/**
 * Implements hook_form_alter().
 */
function farm_pcsc_form_alter(&$form, FormStateInterface $form_state, $form_id) {

  // Don't allow changing "Field" or "Plan" in our plan_record entity forms.
  if (in_array($form_id, array_map(function ($bundle) {
    return 'plan_record_' . $bundle . '_edit_form';
  }, array_keys(farm_pcsc_plan_record_bundle_classes())))) {
    $form['plan']['#access'] = FALSE;
    $form['pcsc_field']['#access'] = FALSE;
  }
}

/**
 * Implements hook_farm_ui_theme_region_items().
